{
    "manifest_version": 3,
    "name": "Simple Crew Extension",
    "version": "1.0",
    "description": "A simple Chrome extension to demonstrate crew functionality.",
    "permissions": ["storage"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "crew.png"
    }
}
